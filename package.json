{
  "name": "bike-island-server",
  "version": "2.0.0",
  "description": "A Node.js Express server for Bike Island with modular architecture, caching, logging, and testing",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "swagger-autogen": "swagger-autogen ./swagger-output.json ./src/routes/*.js",
    "docker-build": "docker build -t bike-island-server .",
    "docker-run": "docker run -d -p 5000:5000 bike-island-server",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "express",
    "mongodb",
    "redis",
    "swagger",
    "jest",
    "winston",
    "mongoose"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "express-openid-connect": "^2.17.1",
    "redis": "^4.0.0",
    "winston": "^3.3.3",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "csv-parser": "^3.0.0",
    "csvtojson": "^2.0.10",
    "date-fns": "^3.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.2.0",
    "express-winston": "^4.2.0",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "jsonwebtoken": "^9.0.1",
    "module-alias": "^2.2.3",
    "mongodb": "^5.3.0",
    "mongoose": "^7.2.2",
    "multer": "^1.4.5-lts.1",
    "ngrok": "^5.0.0-beta.2",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.14",
    "qs": "^6.13.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "jshint": "^2.13.6",
    "mongodb-memory-server": "^10.0.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0"
  }
}
